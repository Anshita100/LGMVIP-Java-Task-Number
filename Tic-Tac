    private void checkWinner()
    {
        if(jTextField1.getText().equals(jTextField2.getText()) && jTextField1.getText().equals(jTextField3.getText()) &&  jTextField1.getText().isEmpty()==false)
        {
            if(playeroneturn){
            JOptionPane.showMessageDialog(this, "Player 2 is winner..");
            }
            else
            {
             JOptionPane.showMessageDialog(this, "Player 1 is winner..");
            }
            winner=true;
        }
        if(jTextField4.getText().equals(jTextField5.getText()) && jTextField4.getText().equals(jTextField6.getText()) &&  jTextField4.getText().isEmpty()==false)
        {
            if(playeroneturn){
            JOptionPane.showMessageDialog(this, "Player 2 is winner..");
            }
            else
            {
             JOptionPane.showMessageDialog(this, "Player 1 is winner..");
            }
            winner=true;
        }
        if(jTextField7.getText().equals(jTextField8.getText()) && jTextField7.getText().equals(jTextField9.getText()) &&  jTextField7.getText().isEmpty()==false)
        {
            if(playeroneturn){
            JOptionPane.showMessageDialog(this, "Player 2 is winner..");
            }
            else
            {
             JOptionPane.showMessageDialog(this, "Player 1 is winner..");
            }
            winner=true;
        }
        if(jTextField1.getText().equals(jTextField4.getText()) && jTextField1.getText().equals(jTextField7.getText()) &&  jTextField1.getText().isEmpty()==false)
        {
            if(playeroneturn){
            JOptionPane.showMessageDialog(this, "Player 2 is winner..");
            }
            else
            {
             JOptionPane.showMessageDialog(this, "Player 1 is winner..");
            }
            winner=true;
        }
        if(jTextField2.getText().equals(jTextField5.getText()) && jTextField2.getText().equals(jTextField8.getText()) &&  jTextField2.getText().isEmpty()==false)
        {
            if(playeroneturn){
            JOptionPane.showMessageDialog(this, "Player 2 is winner..");
            }
            else
            {
             JOptionPane.showMessageDialog(this, "Player 1 is winner..");
            }
            winner=true;
        }
        if(jTextField3.getText().equals(jTextField6.getText()) && jTextField3.getText().equals(jTextField9.getText()) &&  jTextField3.getText().isEmpty()==false)
        {
            if(playeroneturn){
            JOptionPane.showMessageDialog(this, "Player 2 is winner..");
            }
            else
            {
             JOptionPane.showMessageDialog(this, "Player 1 is winner..");
            }
            winner=true;
        }
        if(jTextField3.getText().equals(jTextField5.getText()) && jTextField3.getText().equals(jTextField7.getText()) &&  jTextField3.getText().isEmpty()==false)
        {
            if(playeroneturn){
            JOptionPane.showMessageDialog(this, "Player 2 is winner..");
            }
            else
            {
             JOptionPane.showMessageDialog(this, "Player 1 is winner..");
            }
            winner=true;
        }if(jTextField1.getText().equals(jTextField5.getText()) && jTextField1.getText().equals(jTextField9.getText()) &&  jTextField1.getText().isEmpty()==false)
        {
            if(playeroneturn){
            JOptionPane.showMessageDialog(this, "Player 2 is winner..");
            }
            else
            {
             JOptionPane.showMessageDialog(this, "Player 1 is winner..");
            }
            winner=true;
        }
        if(winner)
        {
            jTextField1.setText("");
            jTextField2.setText("");
            jTextField3.setText("");
            jTextField4.setText("");
            jTextField5.setText("");
            jTextField6.setText("");
            jTextField7.setText("");
            jTextField8.setText("");
            jTextField9.setText("");
            winner=false;
            playeroneturn=true;
            Playerturntext.setText("Player 1:Your Turn");
        }
    }
    private void jTextField1ActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
    }                                           

    private void jTextField1MouseClicked(java.awt.event.MouseEvent evt) {                                         
        // TODO add your handling code here:
        if(jTextField1.getText().equals("")){    
       if(playeroneturn)
       {    
        jTextField1.setText("0");
        playeroneturn=false;
        Playerturntext.setText("Player 2:Your Turn");
       }
       else
         {    
        jTextField1.setText("X");
        playeroneturn=true;
        Playerturntext.setText("Player 1:Your Turn");
       }
        }
        else
        {
            JOptionPane.showMessageDialog(this, "This area is occupied..");
        }
         checkWinner();  
    }                                        

